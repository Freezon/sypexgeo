    "keywords": ["laravel", "laravel 6+", "geoip", "location", "geolocation"],
	"license": "BSD-2-Clause",
    "authors": [],
    "require": {
        "php": ">=7.2",
        "laravel/framework": "6.* || 7.*",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-simplexml": "*"
    },
    "autoload": {
        "psr-4": {
          "Freez0n\\SypexGeo\\": "src/Freez0n/SypexGeo/"
        }
    },
    "extra": {
