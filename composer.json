{
    "name": "freezon/sypexgeo",
    "description": "Supports GeoIP services (sypexgeo.net).",
    "keywords": ["laravel", "laravel 9", "geoip", "location", "geolocation"],
    "license": "BSD-2-Clause",
    "authors": [],
    "require": {
        "php": ">=8.0",
        "laravel/framework": "9.*",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-simplexml": "*"
    },
    "autoload": {
        "psr-4": {
            "Freez0n\\SypexGeo\\": "src/Freez0n/SypexGeo/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Freez0n\\SypexGeo\\SypexGeoServiceProvider"
            ],
            "aliases": {
                "SypexGeo": "Freez0n\\SypexGeo\\SypexGeo"
            }
        }
    }
}
